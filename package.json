{
  "name": "confirmmodal.js",
  "version": "1.1.6",
  "description": "Confirm Modal for javascript",
  "scripts": {
    "build": "npm run build-browserify && npm run build-lib && npm run build-min",
    "build-browserify": "browserify src/ConfirmModal.js -s ConfirmModal -p [bannerify --file .banner ] -o dist/ConfirmModal.js",
    "build-lib": "babel src --plugins transform-object-assign --out-dir lib --loose all",
    "build-min": "uglifyjs dist/ConfirmModal.js --comments '/!/' -m screw_ie8=true --reserved 'ConfirmModal' -c screw_ie8=true,unused=false -o dist/ConfirmModal.min.js",
    "build-watch": "watchify src/ConfirmModal.js -s ConfirmModal -t [babelify] -o dist/ConfirmModal.js -v",
    "test": "npm run lint && npm run test-unit && npm run test-e2e",
    "test-unit": "mocha --compilers js:babel-core/register test/unit/**/*.spec.js",
    "test-e2e": "wdio wdio.conf.js",
    "lint": "eslint src/ConfirmModal.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrisbenseler/confirmmodal.js.git"
  },
  "keywords": [
    "confirm",
    "modal",
    "javascript"
  ],
  "author": "Christian Benseler",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chrisbenseler/confirmmodal.js/issues"
  },
  "homepage": "https://github.com/chrisbenseler/confirmmodal.js#readme",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.14.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.5.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2015-loose": "^8.0.0",
    "babelify": "^7.3.0",
    "bannerify": "^1.0.1",
    "browserify": "^13.1.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "eslint": "3.12.2",
    "eslint-config-google": "0.7.1",
    "mocha": "3.2.0",
    "nock": "^9.0.2",
    "wdio-mocha-framework": "^0.5.4",
    "wdio-phantomjs-service": "^0.2.2",
    "wdio-spec-reporter": "0.0.5",
    "webdriverio": "^4.5.2"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "uglifyjs": "^2.4.10",
    "watchify": "^3.7.0"
  }
}
