{
  "name": "confirmmodal.js",
  "version": "1.0.2",
  "description": "Confirm Modal for javascript",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-browserify && npm run build-lib",
    "build-browserify": "browserify src/ConfirmModal.js -o dist/ConfirmModal.js",
    "build-lib": "babel src --out-dir lib --loose all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrisbenseler/confirmmodal.js.git"
  },
  "keywords": [
    "confirm",
    "modal",
    "javascript"
  ],
  "author": "Christian Benseler",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chrisbenseler/confirmmodal.js/issues"
  },
  "homepage": "https://github.com/chrisbenseler/confirmmodal.js#readme",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-plugin-transform-es2015-modules-umd": "^6.5.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0"
  },
  "browserify": {
    "transform": [["babelify", { "presets": ["es2015"] }]]
  }
}
